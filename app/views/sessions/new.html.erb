<%= form_tag sessions_path do %>

<div class="row">
  <div class="col-md-6 col-md-offset-3">

<%= label_tag :username %>
<%= text_field_tag :username, (params[:username] unless params[:username].blank?) %>
<%= label_tag :password %>
<%= password_field_tag :password %>

</div>

<%= submit_tag :submit, class: "btn btn-primary" %>
<% end %>

    <p>New user? <%= link_to "Sign up now!", sign_up_path %></p>
 